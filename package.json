    "next-connect": "^0.12.2",
    "react": "18.1.0",
    "react-bootstrap": "^2.4.0",
    "react-dom": "18.1.0",
    "react-redux": "^8.0.0",
    "react-toastify": "^8.2.0",
    "typeorm": "^0.3.6"
  },
  "devDependencies": {
    "@babel/core": "^7.18.5",
    "@babel/plugin-proposal-decorators": "^7.18.2",
    "@popperjs/core": "2.10.2",
